{
	"parser": "babel-eslint",
	"extends": "airbnb-base",
	"plugins": [
		"react",
		"jsx-a11y",
		"import"
	],
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"no-plusplus": 0,
		"no-continue": 0,
		"guard-for-in": 0,
		"linebreak-style": 0,
		"no-restricted-syntax": 0,
		"max-len": 0,
		"no-tabs": 0,
		"no-loop-func": 0,
		"no-underscore-dangle": 0,
		"object-curly-newline": 0,
		"yoda": "off"
	}
}