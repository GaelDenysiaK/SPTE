{
    "name": "SPTE",
    "description": "Affiche quelques erreurs sur les traductions fran√ßaises WordPress",
    "version": "0.9.9.1",
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        },
        "default_title": "SPTE"
    },
    "content_scripts": [{
        "matches": ["https://translate.wordpress.org/*"],
        "js": ["spte.js"]
    }]
}